{
  "realm": "exchange-example",
  "enabled": true,
  "registrationAllowed": true,
  "loginWithEmailAllowed": true,
  "clients": [
    {
      "clientId": "exchange-example-api",
      "name": "Exchange Example API",
      "publicClient": false,
      "secret": "${KC_CLIENT_SECRET}",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "redirectUris": [
        "http://localhost:5000/*",
        "http://localhost:3000/*"
      ],
      "webOrigins": [ "*" ]
    },
	{
      "clientId": "exchange-example-client",
      "name": "Exchange Example Client",
      "publicClient": true,
      "secret": "${KC_CLIENT_SECRET}",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "redirectUris": [
        "http://localhost:4200/*"
      ],
      "webOrigins": [ "*" ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "api.user",
        "composite": false
      },
      {
        "name": "api.admin",
        "composite": false
      }
    ]
  }
}